{
  "resourceType": "StructureDefinition",
  "id": "dpc-profile-attribution-roster",
  "url": "https://dpc.cms.gov/fhir/v1/StructureDefinition/dpc-profile-attribution-roster",
  "name": "DPC Attribution Roster",
  "publisher": "The DPC Team",
  "status": "draft",
  "version": "0.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Group",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Group",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Group",
        "path": "Group"
      },
      {
        "id": "Group.active",
        "path": "Group.active",
        "definition": "Group must be active",
        "fixedBoolean": true
      },
      {
        "id": "Group.type",
        "path": "Group.type",
        "definition": "Group must be person",
        "fixedCode": "person"
      },
      {
        "id": "Group.actual",
