          "discriminator": [
            {
              "type": "value",
              "path": "code"
            }
          ],
          "rules": "open"
        }
      },
      {
        "id": "Group.characteristic.code.coding[x]",
        "path": "Group.characteristic.code.coding",
        "sliceName": "group-characteristic",
        "min": 1
      },
      {
        "id": "Group.characteristic.code.coding[x]:code",
        "path": "Group.characteristic.code.coding.code",
